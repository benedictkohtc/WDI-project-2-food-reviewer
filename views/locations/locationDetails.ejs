<div class="container">
    <% data.forEach(function (element) { %>
        <h1 class="page-header"><%= element.name %></h1>
        <ul class="list-group">
            <li class="list-group-item">
                <%= element.address %>
            </li>
            <li class="list-group-item">
                Reviews:
                <%= element.reviewsArray %>

            </li>
            <li class="list-group-item">
                <h5>
                submitted by:
                <%= element.poster %>
            </h5></li>
            <li class="list-group-item">
                <form action="../reviews/new/<%= element._id %>" method="POST">
                    <div class="form-group">
                        <h3>Submit Your Review</h3>
                        <label for="review">Review</label>
                        <input type="text" class="form-control" id="titleInput" placeholder="Review" name="review">
                    </div>
                    <button type="submit" class="btn btn-default">Add A Review</button>
                </form>
            </li>
            <% }) %>
        </ul>

</div>
